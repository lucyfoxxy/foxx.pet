---
import BaseLayout from '@Templates/BaseLayout.astro';
import ImprintPage from '@Pages/ImprintPage.astro';
import { getPageContent } from '@Content/siteContent';
export const prerender = true;

const content = await getPageContent('imprint');
const meta = { title: content.title, description: content.description };
---
<BaseLayout title={content.title} meta={meta}>
  <ImprintPage />
</BaseLayout>
