---
import Layout from '@Design/Layout.astro';
import ImprintPage from '@Pages/ImprintPage.astro';
import { sitePages } from '@Content/sitePages';
export const prerender = true;

const imprintContent = sitePages.imprint;
const title = imprintContent.meta.title;
const description = imprintContent.meta.description;
---
<Layout {title} meta={{ description }}>
  <ImprintPage />
</Layout>
