---
import Layout from '@Design/Layout.astro';
import GalleryOverviewPage from '@Pages/Gallery/OverviewPage.astro';
import { siteContent } from '@Content/site';

export const prerender = true;

const { meta, categories } = siteContent.pages.galleries;
const overviewCategories = categories.map(({ slug, title }) => ({ slug, title }));
---
<Layout {meta}>
  <GalleryOverviewPage categories={overviewCategories} />
</Layout>