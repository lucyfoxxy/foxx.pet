---
import Layout from '@Design/Layout.astro';
import StoriesPage from '@Pages/StoriesPage.astro';
import { sitePages } from '@Content/sitePages';
export const prerender = true;

const storiesContent = sitePages.stories;
const title = storiesContent.meta.title;
const description = storiesContent.meta.description;
---
<Layout {title} meta={{ description }}>
  <StoriesPage />
</Layout>
