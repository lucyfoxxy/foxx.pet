---
export interface Props {
  kind: string;
  size?: number | string;
  stroke?: number;
}
const { kind, size = 64, stroke = 2 } = Astro.props;
---

<svg
  width="1.5em"
  height="1.5em"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width={stroke}
  stroke-linecap="round"
  stroke-linejoin="round"
  aria-hidden="true"
  focusable="false"
>
  {kind === 'telegram' && (
    <>
      <path d="m21.5 3.5-19 8.3a.5.5 0 0 0 .03.92l5.74 2.23a.5.5 0 0 1 .29.29l2.58 6.7a.5.5 0 0 0 .9.06l3.16-5.61a.5.5 0 0 1 .63-.22l4.52 1.82a.5.5 0 0 0 .68-.37l1.76-13.46a.5.5 0 0 0-.79-.47Z" />
      <path d="m10.5 15.5 11-12" />
    </>
  )}

  {kind === 'mail' && (
    <>
      <rect x="3" y="5" width="18" height="14" rx="3" />
      <path d="m3 8 9 6 9-6" />
      <path d="m6 19 5-5" />
      <path d="m18 19-5-5" />
    </>
  )}

  {kind === 'facebook' && (
    <>
      <circle cx="12" cy="12" r="10" />
      <path fill="currentColor" stroke="none"
        d="M13 8.5h2V6.5h-2c-1.9 0-3.5 1.6-3.5 3.5v1.5H8v2h1.5V18h2v-4.5H14l.8-2h-3.3v-1c0-.6.5-1 1-1Z" />
    </>
  )}

  {kind === 'instagram' && (
    <>
      <rect x="3.5" y="3.5" width="17" height="17" rx="5" />
      <circle cx="12" cy="12" r="4" />
      <circle cx="17.2" cy="6.8" r="1.3" />
    </>
  )}

  {kind === 'link' && (
    <>
      <path d="M9 7h-2a4 4 0 0 0 0 8h2" />
      <path d="M15 7h2a4 4 0 0 1 0 8h-2" />
      <line x1="8" y1="12" x2="16" y2="12" />
    </>
  )}

  {kind === 'furaffinity' && (
      <g
        fill="currentColor"
        stroke="none"
        transform="translate(12 12) scale(1.12) translate(-12 -12)"
      >
        <circle cx="7.8" cy="9.0" r="1.5" />
        <circle cx="10.6" cy="7.1" r="1.8" />
        <circle cx="13.4" cy="7.1" r="1.8" />
        <circle cx="16.2" cy="9.0" r="1.5" />
        <path
          d="
            M9.5 11.2
            c.9-1.3 2.1-2.1 3.5-2.1
            s2.6.8 3.5 2.1
            c1.7 2.3 1.9 5.1.3 6.6
            -.9 1-2.1 1.6-3.8 1.6
            s-2.9-.6-3.8-1.6
            c-1.6-1.5-1.4-4.3.3-6.6Z"
        />
      </g>
  )}

  {kind === 'fox' && (
    <>
      <path d="M4 8.5 12 5l8 3.5v5.5c0 4.2-3.2 7.2-8 9-4.8-1.8-8-4.8-8-9Z" />
      <path d="M4 8.5 8 15" />
      <path d="M20 8.5 16 15" />
      <path d="m9.5 15.5 2.5-1.5 2.5 1.5" />
      <path d="M9 18c1 .9 2.2 1.4 3 1.4s2-.5 3-1.4" />
    </>
  )}

  {kind === 'rocket' && (
    <>
      <path d="M12 3c2.8 2.4 4.5 6.7 4.5 10.1a6.5 6.5 0 0 1-9 5.8c-.5 1.3-1.6 2.6-3 3.6 0-1.8.4-3.5 1-4.7-1.7-.9-3-2.2-3-4.7C2.5 9.7 7 5.2 12 3Z" />
      <path d="M12 13.5v7" />
      <path d="m8.5 13-4 4.5" />
      <path d="m15.5 13 4 4.5" />
      <circle cx="12" cy="8.5" r="1.6" />
    </>
  )}

  {kind === 'cookie' && (
    <>
      <path d="M20 12a8 8 0 1 1-8-8 3 3 0 0 0 3 3 3 3 0 0 0 3 3 3 3 0 0 0 2 2Z" />
      <circle cx="9" cy="9.5" r="1" fill="currentColor" stroke="none" />
      <circle cx="15.5" cy="14" r="1" fill="currentColor" stroke="none" />
      <circle cx="11" cy="15" r="1" fill="currentColor" stroke="none" />
      <circle cx="14" cy="9.5" r="1" fill="currentColor" stroke="none" />
    </>
  )}

  {kind === 'planet' && (
    <>
      <circle cx="12" cy="12" r="5" />
      <path d="M3 14c4.5-3.5 13-6.5 18-5 1 .3 1.6 1 .9 1.8-3.3 3.7-11.5 7.1-17.2 6.8-1.5-.1-2.2-1.3-1.1-2.6Z" />
    </>
  )}

  {kind === 'palette' && (
    <>
      <path d="M20 13.5c0 4.5-3.6 6.5-6 6.5-1.1 0-2-.8-2-1.7s.8-1.8 1.8-2c.8-.2 1.2-.6 1.2-1.1 0-2-2.6-2.2-4.6-2.2-3.4 0-6.4-2.3-6.4-6 0-3.7 3-6 7-6 5.3 0 9 4.2 9 9.5Z" />
      <circle cx="8" cy="8" r="1" fill="currentColor" stroke="none" />
      <circle cx="11" cy="5.5" r="1" fill="currentColor" stroke="none" />
      <circle cx="14.5" cy="7" r="1" fill="currentColor" stroke="none" />
      <circle cx="16" cy="10.5" r="1" fill="currentColor" stroke="none" />
    </>
  )}

  {kind === 'sparkles' && (
    <>
      <path d="M12 4.5 13.8 8l3.7 1.2-3.7 1.3-1.8 3.5-1.8-3.5L6.5 9.2 10.2 8Z" />
      <path d="M6 4 7 6.2 9.2 7.2 7 8.2 6 10.5 5 8.2 2.8 7.2 5 6.2Z" />
      <path d="M18 14l.8 1.7 1.7.8-1.7.8-.8 1.7-.8-1.7-1.7-.8 1.7-.8Z" />
    </>
  )}

  {kind === 'crystalBall' && (
    <>
      <circle cx="12" cy="10" r="6" />
      <path d="M6 21c1.5-1.5 3.6-2.3 6-2.3s4.5.8 6 2.3" />
      <path d="M8.5 17.5c1-.7 2.2-1.1 3.5-1.1s2.5.4 3.5 1.1" />
      <path d="M9.5 10.5c0-1.4 1.1-2.5 2.5-2.5" />
    </>
  )}

</svg>
